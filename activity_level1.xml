<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/level1Activity"
    android:background="@drawable/background">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:id="@+id/textView3"
        android:textColor="#ffffff"
        android:paddingTop="2dp"
        android:paddingLeft="5dp"
        android:background="@drawable/level_buttons"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />

    <TextView
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:id="@+id/textViewLevel1"
        android:text="Level 1"
        android:paddingLeft="40dp"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:textStyle="italic"
        android:typeface="serif"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        />

    <TextView
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:id="@+id/textViewScore"
        android:text="Score: 0"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:textStyle="italic"
        android:typeface="serif"
        android:padding="10dp"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="65dp"
        android:layout_height="50dp"
        android:id="@+id/timeText"
        android:text="Time:"
        android:textSize="20sp"
        android:paddingTop="10dp"
        android:textColor="#ffffff"
        android:textStyle="italic"
        android:typeface="serif"
        android:layout_alignTop="@+id/textViewScore"
        android:layout_alignRight="@+id/textViewScore"
        android:layout_alignEnd="@+id/textViewScore" />
    <TextView
        android:layout_width="75dp"
        android:layout_height="50dp"
        android:id="@+id/time"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:textStyle="italic"
        android:typeface="serif"
        android:paddingTop="12dp"
        android:layout_alignTop="@+id/textViewScore"
        android:layout_toRightOf="@+id/timeText"
        android:layout_toEndOf="@+id/timeText" />

    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum1"
        android:background="#ccff33"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="60dp"
        android:layout_below="@+id/textViewScore"
       >

        <Button
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:id="@+id/Num4"
            android:text="4"
            android:textSize="20sp"
            android:background="#669900"
            />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum2"
        android:tag="2"
        android:background="#ccff33"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_gravity="right|top"
        android:layout_alignTop="@+id/frameNum1"
        android:layout_toRightOf="@+id/frameNum1"
        >
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum3"
        android:tag="3"
        android:background="#ccff33"
        android:layout_gravity="right|top"
        android:layout_marginBottom="5dp"
        android:layout_alignTop="@+id/frameNum2"
        android:layout_toRightOf="@+id/frameNum2"
        />

    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum4"
        android:tag="4"
        android:background="#ccff33"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="60dp"
        android:layout_below="@+id/frameNum1"
        >
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum5"
        android:tag="5"
        android:background="#ccff33"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_below="@+id/frameNum2"
        android:layout_toRightOf="@+id/frameNum1"
        android:layout_gravity="right|top"
        >
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum6"
        android:tag="6"
        android:background="#ccff33"
        android:layout_marginBottom="5dp"
        android:layout_below="@+id/frameNum3"
        android:layout_toRightOf="@+id/frameNum5"
        >
        <Button
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:id="@+id/Num1"
            android:text="1"
            android:textSize="20sp"
            android:background="#669900"
            />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum7"
        android:tag="7"
        android:background="#ccff33"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="60dp"
        android:layout_below="@+id/frameNum4"
        >
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum8"
        android:tag="8"
        android:background="#ccff33"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="5dp"
        android:layout_toRightOf="@+id/frameNum7"
        android:layout_below="@+id/frameNum5"
        >
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/frameNum9"
        android:tag="9"
        android:background="#ccff33"
        android:layout_toRightOf="@+id/frameNum8"
        android:layout_below="@+id/frameNum6"
        >
    </RelativeLayout>
    <Button
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/Num5"
        android:tag="5"
        android:text="5"
        android:textSize="20sp"
        android:background="#ccff33"
        android:layout_alignTop="@+id/Num8"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        />
    <Button
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/Num8"
        android:tag="3"
        android:text="8"
        android:textSize="20sp"
        android:background="#ccff33"
        android:layout_alignTop="@+id/Num7"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
         />
    <Button
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/Num7"
        android:tag="8"
        android:text="7"
        android:textSize="20sp"
        android:background="#ccff33"
        android:layout_alignTop="@+id/Num9"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
         />
    <Button
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:text="9"
        android:tag="4"
        android:textSize="20sp"
        android:id="@+id/Num9"
        android:background="#ccff33"
        android:layout_alignTop="@+id/Num2"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        />
    <Button
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/Num6"
        android:tag="9"
        android:text="6"
        android:textSize="20sp"
        android:background="#ccff33"
        android:layout_alignTop="@+id/Num3"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        />
    <Button
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/Num3"
        android:tag="2"
        android:text="3"
        android:textSize="20sp"
        android:background="#ccff33"
        android:layout_alignTop="@+id/Num2"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
         />
    <Button
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/Num2"
        android:tag="7"
        android:text="2"
        android:textSize="20sp"
        android:background="#ccff33"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
         />
</RelativeLayout>